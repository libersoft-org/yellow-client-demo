{
	"name": "yellow-client-web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"liveserver": "vite build && vite preview --host 0.0.0.0",
		"resurrect": "npm install && vite build && vite preview --host 0.0.0.0",
